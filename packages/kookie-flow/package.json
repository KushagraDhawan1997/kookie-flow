{
  "name": "@kushagradhawan/kookie-flow",
  "version": "0.0.1-alpha.0",
  "description": "WebGL-native node graph library. React Flow's ergonomics, GPU-rendered for performance at scale.",
  "author": "Kushagra Dhawan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KushagraDhawan1997/kookie-flow"
  },
  "keywords": [
    "react",
    "node-graph",
    "webgl",
    "three.js",
    "react-three-fiber",
    "node-editor",
    "workflow",
    "diagram"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "zustand": "^5.0.0"
  },
  "peerDependencies": {
    "@react-three/drei": ">=9.0.0",
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@react-three/drei": "^9.117.0",
    "@react-three/fiber": "^8.17.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.169.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "three": "^0.170.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
