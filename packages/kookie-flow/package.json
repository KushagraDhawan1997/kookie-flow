{
  "name": "@kushagradhawan/kookie-flow",
  "version": "0.0.1-alpha.0",
  "description": "WebGL-native node graph library. React Flow's ergonomics, GPU-rendered for performance at scale.",
  "author": "Kushagra Dhawan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KushagraDhawan1997/kookie-flow"
  },
  "keywords": [
    "react",
    "node-graph",
    "webgl",
    "three.js",
    "react-three-fiber",
    "node-editor",
    "workflow",
    "diagram"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugins": {
      "import": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/plugins/index.js"
      },
      "require": {
        "types": "./dist/plugins/index.d.cts",
        "default": "./dist/plugins/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "fonts"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "generate:fonts": "node scripts/generate-fonts.js"
  },
  "dependencies": {
    "zustand": "^5.0.10"
  },
  "peerDependencies": {
    "@kushagradhawan/kookie-ui": ">=0.1.147",
    "@react-three/drei": ">=10.0.0",
    "@react-three/fiber": ">=9.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "three": ">=0.170.0"
  },
  "peerDependenciesMeta": {
    "@kushagradhawan/kookie-ui": {
      "optional": true
    }
  },
  "devDependencies": {
    "@kushagradhawan/kookie-ui": "^0.1.147",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.182.0",
    "msdf-bmfont-xml": "^2.8.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "three": "^0.182.0",
    "tsup": "^8.3.0",
    "typescript": "^5.9.0"
  }
}
